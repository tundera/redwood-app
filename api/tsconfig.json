{
  "extends": "../tsconfig.json",
	"compilerOptions": {
		"noEmit": true,
		"allowJs": true,
		"esModuleInterop": true,
		"target": "esnext",
		"module": "esnext",
		"moduleResolution": "node",
    "resolveJsonModule": true,
		"baseUrl": ".",
		"paths": {
			"src/*": [
				"src/*"
			]
		},
		"typeRoots": [
			"node_modules/@types"
		],
		"types": []
	},
	"include": [
		"src",
	]
}
